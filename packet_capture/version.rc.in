#include <windows.h>

#define VER_FILEVERSION @VERSION_MAJOR@.@VERSION_MINOR@.@VERSION_TYPE@.@VERSION_BUILD@
#define VER_FILEVERSION_STR "@VERSION_MAJOR@.@VERSION_MINOR@.@VERSION_TYPE@.@VERSION_BUILD@\0"

#define VER_PRODUCTVERSION @VERSION_MAJOR@.@VERSION_MINOR@.@VERSION_TYPE@.@VERSION_BUILD@
#define VER_PRODUCTVERSION_STR "@VERSION_MAJOR@.@VERSION_MINOR@.@VERSION_TYPE@.@VERSION_BUILD@\0"

#define VER_COMPANYNAME_STR "TMG"
#define VER_FILEDESCRIPTION_STR "PCAP-based packet capture mechanism"
#define VER_INTERNALNAME_STR "PCAP-based packet capture mechanism"
#define VER_LEGALCOPYRIGHT_STR "Â© 2018 TMG"
#define VER_LEGALTRADEMARKS1_STR " "
#define VER_LEGALTRADEMARKS2_STR " "
#define VER_PRODUCTNAME_STR "packet_capture"
#define VS_FFI_FILEFLAGSMASK 0x0000003FL
#define VER_PRIVATEBUILD 0
#define VER_PRERELEASE 0
#define VER_ORIGINALFILENAME_STR "packet_capture"

#ifndef DEBUG
#define VER_DEBUG	0
#else
#define VER_DEBUG	VS_FF_DEBUG
#endif

VS_VERSION_INFO		VERSIONINFO
